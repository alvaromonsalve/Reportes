	</parameter>
	<queryString>
		<![CDATA[SELECT
    CONCAT(`info_paciente`.`tipo_doc`,' - ', `info_paciente`.`num_doc`) AS `identificacion`
    , `info_paciente`.`hc`
    , CONCAT(`info_paciente`.`nombre1`,' ', `info_paciente`.`nombre2`,' ', `info_paciente`.`apellido1`,' ', `info_paciente`.`apellido2`) AS `nombrepaciente`
    , `calc_edad`(`info_paciente`.`fecha_nacimiento`) AS `edad`
    , `info_municipios`.`nombre` AS `municipio`
    , `info_entidades`.`nombre_entidad`
    , `info_paciente`.`tipo_afiliacion`
    , `config_decripcion_login`.`nombres`
    , `config_decripcion_login`.`apellidos`
    , `config_decripcion_login`.`ruta_firma`
    , `cm_profesionales`.`tarjeta_profesional`
    , DATE_FORMAT(`hcu_evolucion`.`f_digita`, '%d/%m/%Y') AS `fecha`
FROM
    `database`.`hcu_evolucion`
    INNER JOIN `database`.`info_historiac` ON (`hcu_evolucion`.`id_info_historiac` = `info_historiac`.`id`)
    INNER JOIN `database`.`config_decripcion_login` ON (`info_historiac`.`id_usuario` = `config_decripcion_login`.`id`)
    INNER JOIN `database`.`info_admision` ON (`info_historiac`.`id_infoadmision` = `info_admision`.`id`)
    INNER JOIN `database`.`cm_profesionales` ON (`cm_profesionales`.`id_descripcion_login` = `config_decripcion_login`.`id`)
    INNER JOIN `database`.`info_paciente` ON (`info_admision`.`id_datos_personales` = `info_paciente`.`id`)
    INNER JOIN `database`.`info_entidades` ON (`info_paciente`.`contratante` = `info_entidades`.`id`)
    INNER JOIN `database`.`info_municipios` ON (`info_municipios`.`id` = `info_paciente`.`id_mun_res`)
WHERE (`hcu_evolucion`.`id` =$P{Idevolucion} AND `hcu_evolucion`.`estado` =4);]]>
	</queryString>
