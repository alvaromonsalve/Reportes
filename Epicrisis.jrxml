	<queryString>
		<![CDATA[SELECT
    (CONCAT(`info_paciente`.`nombre1`,' ',`info_paciente`.`nombre2`,' ',`info_paciente`.`apellido1`,' ',`info_paciente`.`apellido2`)) AS NOMBRE
    , `info_paciente`.`estado_civil` AS `ESTADOCIVIL`
    , `info_paciente`.`genero` AS `SEXO`
    , `info_pais`.`nombre` AS `PAISNACIMIENTO`
    , `info_departamentos`.`nombre` AS `DEPRESIDENCIA`
    , `info_paciente`.`direccion_ppal` AS `DIRECCION`
    , `info_paciente`.`ocupacion` AS `OCUPACION`
    , `info_entidades`.`nombre_entidad` AS `NOMBREENTIDAD`
    , `info_admision`.`edad` AS `EDAD`
    , `info_paciente`.`fecha_nacimiento` AS `FECHANACIMIENTO`
    , `info_pais_1`.`nombre` AS `PAISRESIDENCIA`
    , `info_municipios`.`nombre` AS `MUNICIPIORESIDENCIA`
    , `info_paciente`.`telefono` AS `TELEFONO`
    , `info_paciente`.`estrato_se` AS `ESTRATO`
    , `info_paciente`.`nivel` AS `NIVEL`
    , CONCAT(`info_paciente`.`tipo_doc`,' ', `info_paciente`.`num_doc`) AS DOCUMENTO
    , `info_paciente`.`barrio_o_vereda` AS `BARRIO`
    , `info_paciente`.`num_celular` AS `CELULAR`
    , `info_paciente`.`zona_res` AS `ZONA`
    , `info_paciente`.`tipo_afiliacion` AS `TIPOAFILIACION`
    ,(CONCAT( `config_decripcion_login`.`nombres`, ' ', `config_decripcion_login`.`apellidos`)) AS `NombreMedico`
    , `cm_profesionales`.`tarjeta_profesional` AS `RegistroMEdico`
    , `config_decripcion_login`.`ruta_firma` AS `Firma`
    , `info_admision`.`id`
    , `info_paciente`.`hc`
    , `info_historiac`.`motivo_consulta`
    , `info_historiac`.`causa_externa`
    , `info_historiac`.`nivel_triaje`
    , `info_historiac`.`alergias`
    , `info_historiac`.`ingresos_previos`
    , `info_historiac`.`traumatismos`
    , `info_historiac`.`tratamientos`
    , `info_historiac`.`dm`
    , `info_historiac`.`hta`
    , `info_historiac`.`dislipidemia`
    , `info_historiac`.`desc_hdd`
    , `info_historiac`.`tabaco`
    , `info_historiac`.`alcohol`
    , `info_historiac`.`droga`
    , `info_historiac`.`otros_habitos`
    , `info_historiac`.`situacion_basal`
    , `info_historiac`.`ant_familiares`
    , `info_historiac`.`enfermedad_actual`
    , `info_hc_expfisica`.`ta`
    , `info_hc_expfisica`.`tam`
    , `info_hc_expfisica`.`fc`
    , `info_hc_expfisica`.`fr`
    , `info_hc_expfisica`.`t`
    , `info_hc_expfisica`.`sao2`
    , `info_hc_expfisica`.`pvc`
    , `info_hc_expfisica`.`pic`
    , `info_hc_expfisica`.`peso`
    , `info_hc_expfisica`.`talla`
    , `info_hc_expfisica`.`otros`
    , `info_hc_expfisica`.`aspectogeneral`
    , `info_hc_expfisica`.`cara`
    , `info_hc_expfisica`.`cardio`
    , `info_hc_expfisica`.`respiratorio`
    , `info_hc_expfisica`.`gastro`
    , `info_hc_expfisica`.`renal`
    , `info_hc_expfisica`.`hemato`
    , `info_hc_expfisica`.`endo`
    , `info_hc_expfisica`.`osteo`
    , `info_historiac`.`hallazgo`
    , `info_admision`.`fecha_ingreso`,
    static_cie101.id AS static_cie101_id,
static_cie101.codigo AS codigo1,
static_cie101.descripcion AS descripcion1,
static_cie102.id AS static_cie102_id,
static_cie102.codigo AS codigo2,
static_cie102.descripcion AS descripcion2,
static_cie103.id AS static_cie103_id,
static_cie103.codigo AS codigo3,
static_cie103.descripcion AS descripcion3,
static_cie104.id AS static_cie104_id,
static_cie104.codigo AS codigo4,
static_cie104.descripcion AS descripcion4,
static_cie105.id AS static_cie105_id,
static_cie105.codigo AS codigo5,
static_cie105.descripcion AS descripcion5, `info_admision`.`estadoIngreso`
    , `info_admision`.`mediodeingreso`
    , `info_admision`.`hora_ingreso`
    , `info_otros_datos_admision`.`materna`
    , `info_otros_datos_admision`.`exposicion_dolor`
    , `info_otros_datos_admision`.`estado` AS `Estadomater`
    , `info_admision`.`causaExterna`
FROM
    `database`.`info_historiac`
    INNER JOIN `database`.`info_admision`
        ON (`info_historiac`.`id_infoadmision` = `info_admision`.`id`)
    INNER JOIN `database`.`config_decripcion_login`
        ON (`info_historiac`.`id_usuario` = `config_decripcion_login`.`id`)
    INNER JOIN `database`.`info_paciente`
        ON (`info_admision`.`id_datos_personales` = `info_paciente`.`id`)
    INNER JOIN `database`.`info_entidades`
        ON (`info_paciente`.`contratante` = `info_entidades`.`id`)
    INNER JOIN `database`.`info_camas`
        ON (`info_camas`.`id_info_historiac` = `info_historiac`.`id`)
    INNER JOIN `database`.`config_camas`
        ON (`info_camas`.`id_config_camas` = `config_camas`.`id`)
    INNER JOIN `database`.`cm_profesionales`
        ON (`cm_profesionales`.`id_descripcion_login` = `config_decripcion_login`.`id`)
    INNER JOIN `database`.`info_hc_expfisica`
        ON (`info_hc_expfisica`.`id_infohistoriac` = `info_historiac`.`id`)
    INNER JOIN `database`.`static_cie10` AS static_cie101
        ON (`info_historiac`.`diagnostico` = `static_cie101`.`id`)
