		</parameter>
		<queryString>
			<![CDATA[SELECT
     info_posologia_hcu.`dosis_n`,
     info_posologia_hcu.`dosis_u` AS dosis_u,
     info_posologia_hcu.`administracion` AS administracion,
     sum_suministro.`id` AS id_sum_suministro,
     sum_atc_principioactivo.`nombre` AS principioact,
     sum_atc_presentacionfarmaceutica.`nombre` AS presfarmaceutica,
     sum_suministro.`concentracion` AS concentracion,
     sum_suministro.`viaadministracion` AS viaadministracion
FROM
     `database`.`info_historiac` info_historiac INNER JOIN `database`.`Config_decripcion_login` Config_decripcion_login ON info_historiac.`id_usuario` = Config_decripcion_login.`id`
     INNER JOIN `database`.`info_posologia_hcu` info_posologia_hcu ON info_historiac.`id` = info_posologia_hcu.`id_historiac`
     INNER JOIN `database`.`info_Admision` info_Admision ON info_historiac.`id_infoadmision` = info_Admision.`id`
     INNER JOIN `database`.`info_Paciente` info_Paciente ON info_Admision.`id_datos_personales` = info_Paciente.`id`
     INNER JOIN `database`.`info_entidades` info_entidades ON info_Admision.`id_entidad_admision` = info_entidades.`id`
     INNER JOIN `database`.`sum_suministro` sum_suministro ON info_posologia_hcu.`id_suministro` = sum_suministro.`id`
     INNER JOIN `database`.`sum_atc_principioactivo` sum_atc_principioactivo ON sum_suministro.`id_pricipioactivo` = sum_atc_principioactivo.`id`
     INNER JOIN `database`.`sum_atc_presentacionfarmaceutica` sum_atc_presentacionfarmaceutica ON sum_suministro.`id_presentacionfarmaceutica` = sum_atc_presentacionfarmaceutica.`id`
WHERE `info_historiac`.id = $P{id_HistoriaC}]]>
