		</parameter>
		<queryString>
			<![CDATA[SELECT
    `config_cups`.`codigo`
    , `config_cups`.`de_subcategoria`
    , `hcu_evo_orden_procedimiento_desc`.`observacion`
FROM
    `database`.`hcu_evo_orden_procedimiento_desc`
    INNER JOIN `database`.`hcu_evo_orden_procedimiento`
        ON (`hcu_evo_orden_procedimiento_desc`.`id_evu_orden_procedimiento` = `hcu_evo_orden_procedimiento`.`id`)
    INNER JOIN `database`.`hcu_evolucion`
        ON (`hcu_evo_orden_procedimiento`.`id_evu` = `hcu_evolucion`.`id`)
    INNER JOIN `database`.`config_cups`
        ON (`config_cups`.`id` = `hcu_evo_orden_procedimiento_desc`.`id_config_cups`)
WHERE (`hcu_evolucion`.`id` =$P{idevo}
    AND `hcu_evolucion`.`estado` =2
    AND `hcu_evo_orden_procedimiento`.`id` =$P{idconsecutivo} AND config_cups.`id_estructura_cups` = '17');]]>
		</queryString>
		<field name="codigo" class="java.lang.String"/>
		<field name="de_subcategoria" class="java.lang.String"/>
		<field name="observacion" class="java.lang.String"/>
	</subDataset>
